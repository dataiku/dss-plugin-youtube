                },
                {
                    "value": "snippet",
                    "label": "Snippet"
                },
                {
                    "value": "replies",
                    "label": "Replies"
                }
            ]
        },
        {
            "name": "subscriptions_part",
            "label": "Parts",
            "type": "MULTISELECT",
            "visibilityCondition": "['subscriptions'].indexOf(model.endpoint) >= 0",
            "defaultValue": [],
