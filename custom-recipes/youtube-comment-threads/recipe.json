{
    "meta": {
        "label": "Youtube comments threads",
        "description": "Retrieve comments threads for channels or videos",
        "icon": "icon-play"
    },
    "kind": "PYTHON",
    "inputRoles": [
        {
            "name": "input_A_role",
            "label": "Dataset containing Youtube ids",
            "description": "",
            "arity": "UNARY",
            "required": true,
            "acceptsDataset": true
        }
    ],

    "outputRoles": [
        {
            "name": "youtube_output",
            "label": "main output displayed name",
            "description": "what main output means",
            "arity": "UNARY",
            "required": true,
            "acceptsDataset": true
        }
    ],
    "params": [
        {
            "name": "access_type",
            "label": "Access type",
            "type": "SELECT",
            "defaultValue": "token_access",
            "selectChoices": [
                {
                    "value": "token_access",
                    "label": "API Key"
                },
                {
                    "value": "oauth_credentials",
                    "label": "Google Single Sign On"
                }
            ]
        },
        {
            "name": "token_access",
            "label": "API key access",
            "type": "PRESET",
            "parameterSetId": "api-key",
            "visibilityCondition": "model.access_type == 'token_access'"
        },
        {
            "name": "oauth_credentials",
            "label": "Google Single Sign On",
            "type": "PRESET",
            "parameterSetId": "oauth-credentials",
            "visibilityCondition": "model.access_type == 'oauth_credentials'"
        },
        {
            "name": "edge_name",
            "label": "Item type",
            "type": "SELECT",
            "defaultValue": "commentThreads",
            "visibilityCondition": false,
            "selectChoices": [
                {
                    "value": "playlists",
                    "label": "Playlists"
                },
                {
                    "value": "videos",
                    "label": "Video"
                },
                {
                    "value": "channels",
                    "label": "Channels"
                },
                {
                    "value": "comments",
                    "label": "Comments"
                },
                {
                    "value": "commentThreads",
                    "label": "Comment threads"
                },
                {
                    "value": "subscriptions",
                    "label": "Subscriptions"
                }
            ]
        },
        {
            "name": "part",
            "label": "Parts",
            "type": "MULTISELECT",
            "visibilityCondition": "['playlists','videos'].indexOf(model.edge_name) >= 0",
            "selectChoices": [
                {
                    "value": "contentDetails",
                    "label": "Content details"
                },
                {
                    "value": "id",
                    "label": "ID"
                },
                {
                    "value": "localizations",
                    "label": "Localizations"
                },
                {
                    "value": "player",
                    "label": "Player"
                },
                {
                    "value": "snippet",
                    "label": "Snippet"
                },
                {
                    "value": "status",
                    "label": "Status"
                }
            ]
        },
        {
            "name": "part",
            "label": "Parts",
            "type": "MULTISELECT",
            "visibilityCondition": "model.edge_name == 'channels'",
            "selectChoices": [
                {
                    "value": "auditDetails",
                    "label": "Audit details"
                },
                {
                    "value": "brandingSettings",
                    "label": "Branding settings"
                },
                {
                    "value": "contentDetails",
                    "label": "Content details"
                },
                {
                    "value": "contentOwnerDetails",
                    "label": "Content owner details"
                },
                {
                    "value": "id",
                    "label": "ID"
                },
                {
                    "value": "invideoPromotion",
                    "label": "In video promotion"
                },
                {
                    "value": "localizations",
                    "label": "Localizations"
                },
                {
                    "value": "snippet",
                    "label": "Snippet"
                },
                {
                    "value": "statistics",
                    "label": "Statistics"
                },
                {
                    "value": "status",
                    "label": "Status"
                },
                {
                    "value": "topicDetails",
                    "label": "Topic details"
                }
            ]
        },
        {
            "name": "part",
            "label": "Parts",
            "type": "MULTISELECT",
            "visibilityCondition": "['comments'].indexOf(model.edge_name) >= 0",
            "selectChoices": [
                {
                    "value": "id",
                    "label": "ID"
                },
                {
                    "value": "snippet",
                    "label": "Snippet"
                }
            ]
        },
        {
            "name": "part",
            "label": "Parts",
            "type": "MULTISELECT",
            "visibilityCondition": "['commentThreads'].indexOf(model.edge_name) >= 0",
            "selectChoices": [
                {
                    "value": "id",
                    "label": "ID"
                },
                {
                    "value": "snippet",
                    "label": "Snippet"
                },
                {
                    "value": "replies",
                    "label": "Replies"
                }
            ]
        },
        {
            "name": "part",
            "label": "Parts",
            "type": "MULTISELECT",
            "visibilityCondition": "['subscriptions'].indexOf(model.edge_name) >= 0",
            "selectChoices": [
                {
                    "value": "contentDetails",
                    "label": "Content details"
                },
                {
                    "value": "id",
                    "label": "ID"
                },
                {
                    "value": "snippet",
                    "label": "Snippet"
                },
                {
                    "value": "subscriberSnippet",
                    "label": "Subscriber snippet"
                }
            ]
        },
        {
            "name": "search_id_type",
            "label": "Searching by",
            "type": "SELECT",
            "visibilityCondition": "['commentThreads'].indexOf(model.edge_name) >= 0",
            "deaultValue": "video_id",
            "selectChoices": [
                {"value": "video_id", "label":"Video ID"},
                {"value": "channel_id", "label":"Channel ID"},
                {"value": "allThreadsRelatedToChannelId", "label":"All thread related to channel ID"}
            ]
        },
        {
            "name": "id_column_name",
            "label": "ID's column name",
            "type": "STRING",
            "description": "Name of the column containing the playlist IDs to process",
            "defaultValue": "",
            "mandatory": true,
            "visibilityCondition": "model.edge_name == 'playlists'"
        },
        {
            "name": "id_column_name",
            "label": "ID's column name",
            "type": "STRING",
            "description": "Name of the column containing the videos IDs to process",
            "defaultValue": "",
            "mandatory": true,
            "visibilityCondition": "['videos', 'comments', 'subscriptions'].indexOf(model.edge_name) >= 0"
        },
        {
            "name": "id_column_name",
            "label": "ID's column name",
            "type": "STRING",
            "description": "Name of the column containing the channel IDs to process",
            "defaultValue": "",
            "mandatory": true,
            "visibilityCondition": "['channels'].indexOf(model.edge_name) >= 0"
        },
        {
            "name": "id_column_name",
            "label": "ID's column name",
            "type": "STRING",
            "description": "Name of the column containing the IDs to process",
            "defaultValue": "",
            "mandatory": true,
            "visibilityCondition": "['commentThreads'].indexOf(model.edge_name) >= 0"
        }
    ],
    "resourceKeys": []
}
